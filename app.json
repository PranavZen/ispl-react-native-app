{
  "expo": {
    "name": "newexpo",
    "slug": "newexpo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splashs.png",
      "resizeMode": "contain",
      "backgroundColor": "#182046"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cricclubs.isplt10"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icons.png",
        "backgroundColor": "#182046"
      },
      "package": "com.cricclubs.isplt10"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2671040d-364b-4ba2-abfc-0bce401146f3"
      }
    }
  }
}
